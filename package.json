{
  "name": "starter-website-parcel",
  "version": "1.0.0",
  "description": "Simple website starter with Parcel.js",
  "main": "index.js",
  "scripts": {
    "predev": "npm run clean",
    "dev": "parcel src/index.html -p 1234 --open",
    "prebuild": "npm run clean",
    "build": "parcel build src/index.html --out-dir ./dist --public-url . --detailed-report",
    "clean": "shx rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Namide/starter-website-parcel.git"
  },
  "keywords": [
    "parceljs",
    "nodejs",
    "starter",
    "website"
  ],
  "author": "Namide (Damien Doussaud)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Namide/starter-website-parcel/issues"
  },
  "homepage": "https://github.com/Namide/starter-website-parcel#readme",
  "devDependencies": {
    "dat.gui": "^0.7.3",
    "parcel-bundler": "^1.10.3",
    "parcel-plugin-asset-copier": "^1.0.0",
    "postprocessing": "^5.2.2",
    "shx": "^0.3.2",
    "three": "^0.98.0",
    "three-orbit-controls": "^82.1.0"
  },
  "assetsPath": "assets"
}
